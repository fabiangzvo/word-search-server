{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./server",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["./server/*"],
      "@sockets/*": ["./server/sockets/*"],
      "@lib/*": ["./server/lib/*"],
      "@modules/*": ["./server/modules/*"],
    },
  }
  
}
